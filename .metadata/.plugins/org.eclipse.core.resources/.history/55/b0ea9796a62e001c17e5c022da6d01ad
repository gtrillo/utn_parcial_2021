#include <stdio.h>
#include <stdlib.h>
#include "Salon.h"
#include "utn_menu.h"
#define LEN_SALONES 1000
int main(void) {
		Salon salones [LEN_SALONES];
		setbuf (stdout, NULL);
		initSalon(salones, LEN_SALONES);
		utn_menu (salones, LEN_SALONES);
		return EXIT_SUCCESS;
}
